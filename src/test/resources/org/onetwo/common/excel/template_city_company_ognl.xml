<?xml version="1.0" encoding="UTF-8"?>
<template name="报名客户资料">
	<rows>
		<row name="element" type="iterator" datasource="#data" renderHeader="true" > 
			<fields>
				<field name="id" label="主键" rowspan="1"/>
