				<field name="phone" label="电话" rowspan="1"/>
				<field name="postcode" label="邮编" rowspan="1"/>
				<field name="address" label="地址" rowspan="1"/>
				<field name="fax" label="传真" rowspan="1"/>
			</fields>
		</row>
	</rows>
</template>
